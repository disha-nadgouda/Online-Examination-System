                    <Items>
                        <asp:MenuItem NavigateUrl="~/Student/home.aspx" Text="HOME" Value="New Item"></asp:MenuItem>
                        <asp:MenuItem NavigateUrl="~/Student/MyProfile.aspx" Text="MY PROFILE" Value="MY PROFILE"></asp:MenuItem>
